{
	"routes": [
		{
			"route": "/data-api/*",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; connect-src '*'",
				"SOM-TEST": "TestingDataAPI"
			}
		},
		{
			"route": "/api/*",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; connect-src '*'",
				"SOM-TEST": "TestingAPI"
			}
		},
		{
			"route": "/about/*",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; connect-src '*'",
				"SOM-TEST": "TestingAbout"
			}
		},
		{
			"route": "/country/*",
			"allowedRoles": [
				"anonymous"
			]
		},
		{
			"route": "/color/*",
			"allowedRoles": [
				"anonymous"
			]
		},
		{
			"route": "/shape/*",
			"allowedRoles": [
				"anonymous"
			]
		},
		{
			"route": "/anonymous/*",
			"allowedRoles": [
				"anonymous"
			]
		},
		{
			"route": "/authenticated/*",
			"allowedRoles": [
				"authenticated"
			]
		},
		{
			"route": "/admin/*",
			"allowedRoles": [
				"admin"
			]
		}
	],
	"responseOverrides": {
		"404": {
		"rewrite": "/PageNotFound.html",
		"statusCode": 404
		},
		"401": {
		"rewrite": "/NotAuthorized.html",
		"statusCode": 401
		}
	},
	"auth": {
		"identityProviders": {
			"azureActiveDirectory": {
				"registration": {
					"openIdIssuer": "https://login.microsoftonline.com/563337ca-a517-421a-aae0-1aa5b414fd7f/v2.0",
					"clientIdSettingName": "AZURE_CLIENT_ID",
					"clientSecretSettingName": "AZURE_CLIENT_SECRET"
				}
			}
		}
	},
	"globalHeaders": {
		"Access-Control-Allow-Origin": "*",
		"Access-Control-Allow-Methods": "GET, OPTIONS",
		"Cache-Control": "public, no-store, max-age=0, s-maxage=0, no-cache",
		"SOM-TEST": "Testing"
	}
}